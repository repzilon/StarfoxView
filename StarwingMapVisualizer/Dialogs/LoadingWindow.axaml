<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="StarwingMapVisualizer.Dialogs.LoadingWindow"
        Title="LoadingWindow" Width="212" SizeToContent="Height" ShowInTaskbar="False" CanResize="False"
        SystemDecorations="None" WindowStartupLocation="CenterOwner">
    <Button FontFamily="{StaticResource SFFont}" IsEnabled="False" Padding="10" HorizontalContentAlignment="Stretch"
            VerticalContentAlignment="Stretch">
        <StackPanel>
            <TextBlock>LOADING...</TextBlock>
            <ProgressBar Margin="0,5,0,0" IsIndeterminate="True" Height="20" />
        </StackPanel>
    </Button>
</Window>
