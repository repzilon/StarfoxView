<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:screens="clr-namespace:StarwingMapVisualizer.Screens"
        mc:Ignorable="d"
        x:Class="StarwingMapVisualizer.MainWindow"
        Title="Loading Starwing View" Height="768" Width="1024">
    <Window.Template>
        <ControlTemplate TargetType="Window">
            <Grid Background="{TemplateBinding Background}">
                <TransitioningContentControl Content="{TemplateBinding Content}" />
            </Grid>
        </ControlTemplate>
    </Window.Template>
    <screens:LandingScreen/>
</Window>
